{
  "name": "antgonzales.github.io",
  "version": "2.0.1",
  "description": "The professional portfolio of front-end developer Anthony M. Gonzales.",
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "concat": "^3.0.0",
    "node-sass": "^4.5.2",
    "postcss-cli": "^3.1.1",
    "uglify-js": "^2.8.21"
  },
  "scripts": {
    "scss": "node_modules/.bin/node-sass --output-style compressed -o build/css assets/sass",
    "postcss": "node_modules/.bin/postcss build/css/master.css --use autoprefixer -d build/css -x .min.css",
    "build:css": "npm run scss && npm run postcss",
    "concat": "node_modules/.bin/concat -o build/js/base.js assets/js/*",
    "uglify": "node_modules/.bin/uglifyjs build/js/base.js -o build/js/base.min.js",
    "build:js": "npm run concat && npm run uglify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antgonzales/antgonzales.github.io.git"
  },
  "author": "Anthony M. Gonzales",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/antgonzales/antgonzales.github.io/issues"
  },
  "homepage": "https://github.com/antgonzales/antgonzales.github.io"
}
